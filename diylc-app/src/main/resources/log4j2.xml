<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
	<Appenders>
	<!-- org.diylc.appframework.miscutils.NewLogForEachRunFileAppender
	 -->
		<File name="FILEOUT" fileName="/Users/neko/.diylc/log.log"
			append="false">
			<PatternLayout pattern="%d{ABSOLUTE} %5p [%t] %c{1} - %m%n" />
		</File>
		<Console name="STDOUT" target="SYSTEM_OUT">
			<PatternLayout pattern="%d{ABSOLUTE} %5p [%t] %c{1} - %m%n" />
		</Console>
	</Appenders>
	<Loggers>
		<Root level="debug">
			<AppenderRef ref="STDOUT" />
			<AppenderRef ref="FILEOUT" />
		</Root>
	</Loggers>
</Configuration>
